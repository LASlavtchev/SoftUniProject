{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.4.1",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.js",
        "js/bootstrap.min.js",
        "js/bootstrap.js.map",
        "js/bootstrap.min.js.map",
        "js/bootstrap.bundle.js",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.js.map",
        "js/bootstrap.bundle.min.js.map",
        "css/bootstrap.css",
        "css/bootstrap.min.css",
        "css/bootstrap.css.map",
        "css/bootstrap.min.css.map"
      ]
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/"

    },
    {
      "library": "jquery-validate@1.19.1",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "jquery.validate.js",
        "jquery.validate.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.js",
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "library": "animate.css@3.7.2",
      "destination": "wwwroot/lib/animate/"
    },
    {
      "library": "font-awesome@5.12.1",
      "destination": "wwwroot/lib/font-awesome-free/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "webfonts/fa-brands-400.ttf",
        "webfonts/fa-brands-400.woff",
        "webfonts/fa-brands-400.woff2",
        "webfonts/fa-regular-400.ttf",
        "webfonts/fa-regular-400.woff",
        "webfonts/fa-regular-400.woff2",
        "webfonts/fa-solid-900.ttf",
        "webfonts/fa-solid-900.woff",
        "webfonts/fa-solid-900.woff2"
      ]
    },
    {
      "library": "admin-lte@3.0.2",
      "destination": "wwwroot/lib/admin-lte/",
      "files": [
        "css/adminlte.css",
        "css/adminlte.min.css",
        "css/adminlte.css.map",
        "css/adminlte.min.css.map",
        "js/adminlte.js",
        "js/adminlte.min.js",
        "js/adminlte.js.map", 
        "js/adminlte.min.js.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "icheck-bootstrap@3.0.1",
      "destination": "wwwroot/lib/icheck-bootstrap/",
      "files": [
        "icheck-bootstrap.css",
        "icheck-bootstrap.min.css"
      ]
    }
  ]
}
    